{
  "version": 2,
  "buildCommand": "cd frontend-new && npm install && npm run build",
  "outputDirectory": "frontend-new/.next",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://verigrade-backend-production.up.railway.app"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://verigrade-backend-production.up.railway.app/api/$1"
    }
  ]
}